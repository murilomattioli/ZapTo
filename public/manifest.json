{
  "name": "ZapTo - Discador WhatsApp e Telegram",
  "short_name": "ZapTo",
  "id": "ZapTo",
  "start_url": "/",
  "description": "Discador WhatsApp e Telegram",
  "display": "standalone",
  "background_color": "#66cc8a",
  "theme_color": "#66cc8a",
  "orientation": "natural",
  "lang": "pt-BR",
  "dir": "ltr",
  "scope": "/",
  "protocol_handlers": [
    {
      "protocol": "tel",
      "url": "/discador?phone=%s"
    },
    {
      "protocol": "sms",
      "url": "/discador?phone=%s"
    }
  ],
  "related_applications": [
    {
      "platform": "play",
      "url": "https://play.google.com/store/apps/details?id=com.whatsapp"
    },
    {
      "platform": "play",
      "url": "https://play.google.com/store/apps/details?id=org.telegram.messenger"
    }
  ],
  "categories": [
    "utilities",
    "productivity",
    "social",
    "tools",
    "discador",
    "whatsapp",
    "telegram"
  ],
  "serviceworker": {
    "src": "/sw.js",
    "scope": "/"
  },
  "icons": [
    {
      "src": "/ZapTo-192x192.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "/ZapTo-192x192-maskable.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "maskable"
    },
    {
      "src": "/ZapTo-256x256.png",
      "sizes": "256x256",
      "type": "image/png"
    },
    {
      "src": "/ZapTo-256x256-maskable.png",
      "sizes": "256x256",
      "type": "image/png",
      "purpose": "maskable"
    },
    {
      "src": "/ZapTo-512x512.png",
      "sizes": "512x512",
      "type": "image/png"
    },
    {
      "src": "/ZapTo-512x512-maskable.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "maskable"
    }
  ],
  "prefer_related_applications": false,
  "share_target": {
    "method": "GET",
    "action": "/",
    "params": {
      "phone": "phone"
    }
  },
  "manifest_version": 3
}