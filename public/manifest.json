{
  "name": "ZapTo - Discador WhatsApp e Telegram",
  "short_name": "ZapTo",
  "id": "ZapTo",
  "start_url": "/",
  "description": "Discador WhatsApp e Telegram",
  "display": "standalone",
  "background_color": "#66cc8a",
  "theme_color": "#66cc8a",
  "orientation": "natural",
  "lang": "pt-BR",
  "dir": "ltr",
  "scope": "/",
  "protocol_handlers": [
    {
      "protocol": "tel",
      "url": "/?phone=%s"
    },
    {
      "protocol": "sms",
      "url": "/?phone=%s"
    }
  ],
  "intent_filters": [
    {
      "action": "VIEW",
      "href": "/?phone=%s",
      "type": "text"
    }
  ],
  "related_applications": [
    {
      "platform": "play",
      "url": "https://web.whatsapp.com/"
    },
    {
      "platform": "play",
      "url": "https://web.telegram.org/"
    }
  ],
  "categories": [
    "utilities",
    "productivity",
    "social",
    "tools",
    "discador",
    "whatsapp",
    "telegram"
  ],
  "serviceworker": {
    "src": "/sw.js",
    "scope": "/"
  },
  "icons": [
    {
      "src": "/ZapTo-192x192.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "/ZapTo-192x192-maskable.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "maskable"
    },
    {
      "src": "/ZapTo-256x256.png",
      "sizes": "256x256",
      "type": "image/png"
    },
    {
      "src": "/ZapTo-256x256-maskable.png",
      "sizes": "256x256",
      "type": "image/png",
      "purpose": "maskable"
    },
    {
      "src": "/ZapTo-384x384.png",
      "sizes": "384x384",
      "type": "image/png"
    },
    {
      "src": "/ZapTo-384x384-maskable.png",
      "sizes": "384x384",
      "type": "image/png",
      "purpose": "maskable"
    },
    {
      "src": "/ZapTo-512x512.png",
      "sizes": "512x512",
      "type": "image/png"
    },
    {
      "src": "/ZapTo-512x512-maskable.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "maskable"
    }
  ],
  "prefer_related_applications": false,
  "edge_side_panel": {
    "preferred_height": 667,
    "preferred_width": 375
  },
  "share_target": {
    "method": "GET",
    "action": "/",
    "params": {
      "title": "phone"
    }
  },
  "manifest_version": 2,
  "navigation": {
    "start_url": "/",
    "matches": ["tel:*"],
    "targets": [
      {
        "url": "/tel/:phoneNumber",
        "actions": ["open_in_browser"]
      }
    ]
  }
}
