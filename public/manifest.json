{
  "name": "ZapTo - Discador WhatsApp e Telegram",
  "short_name": "ZapTo",
  "description": "Discador WhatsApp e Telegram",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#66cc8a",
  "theme_color": "#66cc8a",
  "orientation": "natural",
  "lang": "pt-BR",
  "dir": "ltr",
  "scope": "/",
  "protocol_handlers": [
    {
      "protocol": "tel",
      "url": "/?phone=%s"
    },
    {
      "protocol": "sms",
      "url": "/?phone=%s"
    }
  ],
  "share_target": {
    "action": "/?phone=%s",
    "method": "GET",
    "enctype": "application/x-www-form-urlencoded",
    "params": {
      "phone": "tel"
    }
  },
  "intent_filters": [
    {
      "action": "VIEW",
      "href": "/?phone=%s",
      "type": "text/x-vcard"
    }
  ],
  "related_applications": [
    {
      "platform": "play",
      "url": "https://web.whatsapp.com/"
    },
    {
      "platform": "play",
      "url": "https://web.telegram.org/"
    }
  ],
  "categories": [
    "utilities",
    "productivity",
    "social",
    "tools",
    "discador",
    "whatsapp",
    "telegram"
  ],
  "serviceworker": {
    "src": "/sw.js",
    "scope": "/"
  },
  "icons": [
    {
      "src": "/ZapTo-192x192.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "/ZapTo-192x192-maskable.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "maskable"
    },
    {
      "src": "/ZapTo-256x256.png",
      "sizes": "256x256",
      "type": "image/png"
    },
    {
      "src": "/ZapTo-384x384.png",
      "sizes": "384x384",
      "type": "image/png"
    },
    {
      "src": "/ZapTo-512x512.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ]
}
